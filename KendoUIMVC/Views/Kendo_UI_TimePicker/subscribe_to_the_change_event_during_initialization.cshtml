@{
    ViewBag.Title = "subscribe_to_the_change_event_during_initialization";
}

<h2>subscribe_to_the_change_event_during_initialization</h2>

<input id="timepicker" />
<script>
    $("#timepicker").kendoTimePicker({
        change: function () {
            var value = this.value();
            console.log(value); //value is the selected date in the timepicker
        }
    });
</script>