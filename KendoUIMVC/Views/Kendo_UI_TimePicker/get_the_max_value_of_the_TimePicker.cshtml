@{
    ViewBag.Title = "get_the_max_value_of_the_TimePicker";
}

<h2>get_the_max_value_of_the_TimePicker</h2>

<input id="timepicker" />
<script>
    $("#timepicker").kendoTimePicker();

    var timepicker = $("#timepicker").data("kendoTimePicker");

    var max = timepicker.max();

    console.log(max);
</script>