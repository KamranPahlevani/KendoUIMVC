@{
    ViewBag.Title = "dates";
}

<h2>dates</h2>

<input id="datePicker" name="datePicker" style="width: 150px" type="date"  />

<script src="~/Scripts/kendo/2013.3.1213/cultures/kendo.culture.en-US.min.js"></script>
<script>
    $("#datePicker").kendoDatePicker({
        dates: [new Date(2000, 10, 10, 10, 0, 0), new Date(2000, 10, 10, 30, 0)] //can manipulate month template depending on this array.
    });
</script>


