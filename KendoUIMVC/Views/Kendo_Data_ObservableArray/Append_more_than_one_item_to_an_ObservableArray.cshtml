@{
    ViewBag.Title = "Append_more_than_one_item_to_an_ObservableArray";
}

<h2>Append_more_than_one_item_to_an_ObservableArray</h2>

<script>
    var array = new kendo.data.ObservableArray([1]);

    var length = array.push(2, 3);

    alert(length); // outputs "3"

    alert(array[1]); // outputs "2"
</script>
