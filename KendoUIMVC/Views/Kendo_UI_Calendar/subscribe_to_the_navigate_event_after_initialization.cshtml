@{
    ViewBag.Title = "subscribe_to_the_navigate_event_after_initialization";
}

<h2>subscribe_to_the_navigate_event_after_initialization</h2>

<div class="k-widget k-calendar" id="calendar"></div>

<script>
    $("#calendar").kendoCalendar({
    });

    // get a reference to the Kendo UI calendar widget
    var calendar = $("#calendar").data("kendoCalendar");
    // bind to the navigate event
    calendar.bind("navigate", function (e) {
        alert('calendar is navigated!');
    });
</script>

