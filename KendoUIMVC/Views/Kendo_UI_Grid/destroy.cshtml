@{
    ViewBag.Title = "destroy";
}

<h2>destroy</h2>

<div id="grid"></div>
<script>
    $("#grid").kendoGrid({
        dataSource: [
            { name: "Jane Doe", age: 30 },
            { name: "John Doe", age: 33 }
        ]
    });
    var grid = $("#grid").data("kendoGrid");
    grid.destroy();
</script>
