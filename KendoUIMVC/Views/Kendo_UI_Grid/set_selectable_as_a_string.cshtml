@{
    ViewBag.Title = "set_selectable_as_a_string";
}

<h2>set_selectable_as_a_string</h2>

<div id="grid"></div>
<script>
    $("#grid").kendoGrid({
        columns: [
          { field: "name" },
          { field: "age" }
        ],
        dataSource: [
          { name: "Jane Doe", age: 30 },
          { name: "John Doe", age: 33 }
        ],
        selectable: "multiple, row"
    });
</script>
