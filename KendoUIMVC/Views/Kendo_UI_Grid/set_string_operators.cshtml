@{
    ViewBag.Title = "set_string_operators";
}

<h2>set_string_operators</h2>

<div id="grid"></div>
<script>
    $("#grid").kendoGrid({
        columns: [
          { field: "name" }
        ],
        dataSource: [
          { name: "Jane Doe" },
          { name: "John Doe" }
        ],
        filterable: {
            operators: {
                string: {
                    eq: "Equal to",
                    neq: "Not equal to"
                }
            }
        }
    });
</script>
