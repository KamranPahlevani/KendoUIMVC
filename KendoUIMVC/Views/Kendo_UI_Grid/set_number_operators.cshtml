@{
    ViewBag.Title = "set_number_operators";
}

<h2>set_number_operators</h2>

<div id="grid"></div>
<script>
    $("#grid").kendoGrid({
        columns: [
          { field: "name" },
          { field: "age" }
        ],
        dataSource: {
            data: [
              { name: "Jane Doe", age: 30 },
              { name: "John Doe", age: 33 }
            ],
            schema: {
                model: {
                    fields: {
                        age: { type: "number" }
                    }
                }
            }
        },
        filterable: {
            operators: {
                number: {
                    eq: "Equal to",
                    neq: "Not equal to"
                }
            }
        }
    });
</script>
