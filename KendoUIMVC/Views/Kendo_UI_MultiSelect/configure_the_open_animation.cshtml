
@{
    ViewBag.Title = "configure_the_open_animation";
}

<h2>configure_the_open_animation</h2>

<select id="multiselect" multiple="multiple">
    <option>Item1</option>
    <option>Item2</option>
</select>
<script>
    $("#multiselect").kendoMultiSelect({
        animation: {
            open: {
                effects: "zoom:in",
                duration: 300
            }
        }
    });
</script>

