
@{
    ViewBag.Title = "get_ul_element";
}

<h2>get_ul_element</h2>

<select id="multiselect" multiple="multiple"></select>
<script>
    $("#multiselect").kendoMultiSelect({
        dataSource: [
          { name: "Apples" },
          { name: "Oranges" }
        ],
        dataTextField: "name",
        dataValueField: "name"
    });

var multiselect = $("#multiselect").data("kendoMultiSelect");

var ul = multiselect.ul;
</script>

