@{
    ViewBag.Title = "dates";
}

<h2>dates</h2>

<input id="dateTimePicker" name="dateTimePicker" style="width: 200px" type="date"  />

<script src="~/Scripts/kendo/2013.3.1213/cultures/kendo.culture.en-US.min.js"></script>
<script>
    $("#dateTimePicker").kendoDateTimePicker({
        dates: [new Date(2000, 10, 10, 10, 0, 0), new Date(2000, 10, 10, 10, 30, 0)] //the drop-down list will consist only two entries ("10:00 AM", "10:30 AM") if selected date is 10/10/2000
    });
</script>

