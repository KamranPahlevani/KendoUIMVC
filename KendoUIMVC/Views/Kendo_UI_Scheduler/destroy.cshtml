@{
    ViewBag.Title = "destroy";
}

<h2>destroy</h2>

<div id="scheduler"></div>
<script>
    $("#scheduler").kendoScheduler({
        date: new Date("2013/6/6"),
        dataSource: [
          {
              id: 1,
              start: new Date("2013/6/6 08:00 AM"),
              end: new Date("2013/6/6 09:00 AM"),
              title: "Interview"
          }
        ]
    });
    var scheduler = $("#scheduler").data("kendoScheduler");
    scheduler.destroy();
</script>