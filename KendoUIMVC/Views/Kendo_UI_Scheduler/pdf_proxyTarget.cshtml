@{
    ViewBag.Title = "pdf_proxyTarget";
}

<h2>pdf_proxyTarget</h2>

<div id="scheduler"></div>
<script>
    $("#scheduler").kendoScheduler({
        toolbar: ["pdf"],
        pdf: {
            forceProxy: true,
            proxyURL: "/save",
            proxyTarget: "_blank"
        },
        date: new Date("2013/6/6"),
        dataSource: [
          {
              id: 1,
              start: new Date("2013/6/6 08:00 AM"),
              end: new Date("2013/6/6 09:00 AM"),
              title: "Interview"
          }
        ]
    });
</script>